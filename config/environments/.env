#
# Sample env config file
#
# See https://reflex-media.github.io/lesgo-docs/0.7.7/#environment-configuration
#

APP_NAME=lesgo
APP_ENV=local
APP_DEBUG=true

AWS_NODEJS_CONNECTION_REUSE_ENABLED=1

AWS_ACCOUNT_REGION=ap-southeast-1
AWS_ACCOUNT_PROFILE=slsDevProfile
AWS_ACCOUNT_ID=

#
# All below config are used in the samples
#

# Sample - Crypto
LESGO_CRYPTO_HASH_ALG=sha256
LESGO_CRYPTO_ENCRYPTION_ALG=aes-256-cbc
LESGO_CRYPTO_ENCRYPTION_SEC=90a4f4097215247b0f6a9917022dcc0a
LESGO_CRYPTO_ENCRYPTION_IVLEN=16

# Sample - JWT
LESGO_JWT_SECRET_KEYS=4e5f6a7b8c9d0e1f:c4156b94c80b7f163feabd4ff268c99e,8c9d6a0e1f4e5f7b:b11ce8995df370a4fd872afb4377b273
LESGO_JWT_ALGORITHM=HS256
LESGO_JWT_EXPIRESIN=1h
LESGO_JWT_ISSUER=lesgo
LESGO_JWT_AUDIENCE=lesgo
LESGO_JWT_VALIDATE_CLAIMS=true

# Sample - VPC (required for OpenSearch)
AWS_VPC_SECURITY_GROUP_ID=
AWS_VPC_SUBNET_1_ID=

# Sample - OpenSearch
AWS_OPENSEARCH_REGION=
AWS_OPENSEARCH_HOST=

# Sample - Cache (Memcache ElastiCache)
AWS_ELASTICACHE_MEMCACHE_HOST=

# Sample - S3
LESGO_AWS_S3_REGION=
LESGO_AWS_S3_BUKCET=
LESGO_AWS_S3_BUCKET_URI=

# Sample - Aurora MySQL RDS Proxy
AWS_RDS_PROXY_HOST=
AWS_RDS_PROXY_USER=
AWS_RDS_PROXY_PASSWORD=
AWS_RDS_PROXY_DATABASE=
